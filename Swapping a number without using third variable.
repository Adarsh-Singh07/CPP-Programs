#include <iostream>

using namespace std;

int main() {
  int a, b;

  cout << "Enter the first number: ";
  cin >> a;
  cout << "Enter the second number: ";
  cin >> b;

  // Swap the numbers using arithmetic operations
  a = a + b;
  b = a - b;
  a = a - b;

  cout << "After swapping: " << endl;
  cout << "a = " << a << endl;
  cout << "b = " << b << endl;

  return 0;
}
