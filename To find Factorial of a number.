#include <iostream>

using namespace std;

int main() {
  int number, result = 1;

  cout << "Enter a number: ";
  cin >> number;

  if (number >= 0) {
    for (int i = 1; i <= number; ++i) {
      result *= i;
    }
    cout << "The factorial of " << number << " is " << result << endl;
  } else {
    cout << "Factorial of negative numbers is undefined." << endl;
  }

  return 0;
}
